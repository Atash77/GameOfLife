pipeline {
	agent any
	stages {
		stage('Compile Stage'){
			steps{
			withMaven(maven: 'Maven 3.6.3'){
			sh 'mvn clean'
				}
 
			}
		}	

		stage('Testing Stage'){
                        steps{
                        withMaven(maven: 'Maven 3.6.3'){
                        sh 'mvn test'
                                }

                        }
		}
   
                  


	}

}
