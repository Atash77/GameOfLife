<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 符号转义说明 &lt; < &gt; > &lt;&gt; <> &amp; & &apos; ' &quot; " <![CDATA[ 
	这里写你的SQL或者符号 ]]> -->
<mapper namespace="com.huilian.hlej.hcf.dao.VendCustInfo">
	<select id="getVendCustQuery" resultType="VendCustInfoVo">
		SELECT t.*
		FROM
		hkf_vend_cust_info t
		<where>
			<if test="vendCode != null and vendCode != ''">
			AND t.vend_code like CONCAT('%','${vendCode}','%' )
			</if>
			<if test="phoneNo != null and phoneNo != ''">
				AND t.phone_no like CONCAT('%','${phoneNo}','%' )
			</if>
			<if test="channelId != null and channelId != ''">
			AND t.channel_id like CONCAT('%','${channelId}','%' )
			</if>
			 <if test="startTime != null and  startTime != ''">
                AND t.regist_date    &gt;=  #{startTime}
            </if>
    		<if test="endTime != null and  endTime != ''">
                AND t.regist_date    &lt;=  #{endTime}
            </if>
			
		</where>
		    ORDER BY  t.regist_date desc
	</select>
	
	<select id="getCommunityName"  resultType="com.huilian.hlej.hcf.vo.CommunityVo"  >
		SELECT
			 community_id  as   communityId,
			 community_name as  communityName
		FROM
			hsh_community     
	</select>
	

</mapper>   
