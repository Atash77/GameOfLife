<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "mybatis-generator-config_1_0.dtd" >  
<generatorConfiguration>
    <properties resource="appbase.properties" />
	<context id="com.huilian.hlej" targetRuntime="org.mybatis.generator.extend.HlejIntrospectedTableMyBatis3Impl">
	   <!--  <plugin type="org.mybatis.generator.plugins.SerializablePlugin"	      <property name="suppressJavaInterface" value="false" />
         </plugin> -->
         <!-- MyBatis3 -->
         <!-- org.mybatis.generator.extend.HlejIntrospectedTableMyBatis3Impl -->
       <property name="scope" value="global"/>
       <!-- TODO 1.指定项目名，用于定位包名com.huilian.hlej.XXX.XXXX.model  -->
       
	   <property name="project" value="hyf.Product"/>
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="false"/>
		</commentGenerator>
       <!-- Connection -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="${jdbc.url}" userId="${jdbc.username}"
			password="${jdbc.password}">
		</jdbcConnection>

		<!--<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:mysql://192.168.30.195:3306:hlej" userId="root"
			password="root">
		</jdbcConnection> -->

		<javaTypeResolver>
			<property name="forceBigDecimals" value="true" />
		</javaTypeResolver>
		
        <!-- java location -->
		<javaModelGenerator targetPackage="com.huilian.hlej.${project}.entity"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="${project}"
			targetProject="src/main/resources/mappings">
			<property name="enableSubPackages" value="true" />
			<property name="nameSuffix" value="Dao" />
		</sqlMapGenerator>

		<javaClientGenerator type="MAPPER" targetPackage="com.huilian.hlej.${project}.dao"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="nameSuffix" value="Dao" />
		</javaClientGenerator>
		<!-- TODO 2.指定表名与实体类名，多个表时必须一一对应  -->
		<table tableName="scheme" 
		domainObjectName="Scheme" 
		enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
<!-- 		  <columnOverride column="create_time" javaType="java.sql.Timestamp" jdbcType="TIMESTAMP"/> -->
<!-- 		  <columnOverride column="update_time" javaType="java.sql.Timestamp" jdbcType=  "TIMESTAMP"/> -->
		</table>
<!--         <table tableName="behavior_define" domainObjectName="BehaviorDefineModel" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table> -->
    </context>
</generatorConfiguration>